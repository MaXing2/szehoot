<script>

$( "#btn5, #btn6, #btn7, #btn8" ).click(function() {
  var checkBox = '#'+$(this).prop("id")+'Check';
  checkBox = $(checkBox);
  if (checkBox.prop('checked')) {
    checkBox.prop('checked', false);
    $(this).removeClass('btn-choosed');
  } else {
    checkBox.prop('checked', true);
    $(this).addClass('btn-choosed');
}
  
});

function countDown(time) {
  fulltime=time;
